{
  "name": "babel-plugin-lodash-template-compile",
  "description": "Babel plugin for compiling lodash templates at compile-time, rather than run-time.",
  "version": "1.0.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rimraf dist",
    "compile": "tsc --skipLibCheck && tstei",
    "compile:watch": "tsc --skipLibCheck --watch",
    "prepublishOnly": "npm run clean && npm run compile"
  },
  "bugs": {
    "url": "https://github.com/fabiospampinato/babel-plugin-lodash-template-compile/issues"
  },
  "license": "MIT",
  "author": {
    "name": "Fabio Spampinato",
    "email": "spampinabio@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fabiospampinato/babel-plugin-lodash-template-compile.git"
  },
  "keywords": [
    "babel",
    "plugin",
    "lodash",
    "template",
    "compile",
    "performance"
  ],
  "dependencies": {
    "@types/lodash": "^4.14.130",
    "lodash": "^4.17.11"
  },
  "devDependencies": {
    "rimraf": "^2.6.0",
    "typescript": "^3.0.3",
    "typescript-transform-export-interop": "^1.0.0"
  }
}
